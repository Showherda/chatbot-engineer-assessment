# Environment variables for ZUS Coffee Chatbot
# Copy this file to .env and fill in your actual API keys

# Groq API Key (required) - Get from https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# # API URLs (for production, use actual service URLs)
# PRODUCTS_API_URL=http://products-api:8000
# OUTLETS_API_URL=http://outlets-api:8000

# Development URLs (when running locally)
PRODUCTS_API_URL=http://localhost:8001
OUTLETS_API_URL=http://localhost:8002

# Optional: Logging level
LOG_LEVEL=INFO
